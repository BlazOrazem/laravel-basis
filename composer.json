{
    "name": "numencode/cms",
    "type": "project",
    "description": "Numencode - Content Management System",
    "keywords": ["numencode", "laravel"],
    "homepage": "https://github.com/BlazOrazem/numencode",
    "license": "MIT",
    "authors": [
      {
        "name": "Blaz Orazem",
        "email": "blaz@orazem.si",
        "homepage": "http://www.orazem.si",
        "role": "Developer"
      }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "laracasts/utilities": "^3.0",
        "laraplus/translatable": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "rap2hpoutre/laravel-log-viewer": "^0.11.0",
        "spatie/laravel-analytics": "^3.3",
        "unisharp/laravel-filemanager": "^1.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Utils/helpers.php",
            "app/Utils/debug.php"
        ],
        "psr-4": {
            "Numencode\\": "app/",
            "Admin\\": "modules/Admin/",
            "Cms\\": "modules/Cms/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
